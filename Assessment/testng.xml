<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="NYTimesSuite" verbose="4" thread-count="2"
       parallel="tests">

    <listeners>
        <listener class-name="com.qa.nytimes.listeners.ExtentReportListener"/>
        <listener class-name="com.qa.nytimes.listeners.AnnotationTransformer"/>
    </listeners>

    <test name="NyTime Test">
        <classes>
            <class name="com.qa.nytimes.tests.HomePageTest" />
        </classes>
    </test>
</suite>